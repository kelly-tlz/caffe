net: "hello_net.prototxt"

display: 10
max_iter: 100

test_iter: 10
test_interval: 10

base_lr: 0.9

lr_policy: "step"
# lr_policy: "inv"
gamma: 0.001
power: 0.75
stepsize: 1000

# snapshot: 1000
# snapshot_prefix: "~/snapshot"

momentum: 0.095
weight_decay: 1.0005
# weight_decay: 0.0005

solver_mode: CPU
